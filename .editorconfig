# For test files we explicitly allow bad code, don't give use warnings now.
[*.test.cs]

# CS8618: Non-nullable field is uninitialized. Consider declaring as nullable.
dotnet_diagnostic.CS8618.severity = none
dotnet_diagnostic.CS8625.severity = none

# CS0649: Will always have its default value null.
dotnet_diagnostic.CS0649.severity = none

# For diagnostics group files we want to ignore any nullable warnings
[*.diagnostics.cs]

dotnet_diagnostic.CS8618.serverity = none